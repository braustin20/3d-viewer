{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "moduleResolution": "Node",
    "noResolve": false,
    "noImplicitAny": false,
    "sourceMap": false,
    "preserveConstEnums":true,
    "declaration": true,
    "isolatedModules": true,
    "lib": [
        "es2017",
        "dom"
    ],
    "rootDir": "src",
    "outDir": "dist"
  },
  "awesomeTypescriptLoaderOptions": {
    "useTranspileModule": true
  },
}